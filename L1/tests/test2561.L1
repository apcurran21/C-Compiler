(@main
(@main
	0 2
	mem rsp 8 <- r13
	mem rsp 0 <- r12
	r12 <- 1900
	r13 <- 2000
	rdi <- r12
	rsi <- r13
	mem rsp -8 <- :ca_ret
	call @create_array 2
	:ca_ret
	rdi <- rax
	rsi <- r12
	rdx <- r13
	mem rsp -8 <- :fa_ret
	call @fill_array 3
	:fa_ret
	rdi <- rax
	call print 1
	r12 <- mem rsp 0
	r13 <- mem rsp 8
	return
)

(@create_array
	2 0
	rsi -= rdi
	rsi >>= 2
	rsi++
	rsi <<= 1
	rsi++
	rdi <- rsi
	rsi <- 1
	call allocate 2
	return
)

(@fill_array
	3 4
	mem rsp 24 <- r15
	mem rsp 16 <- r14
	mem rsp 8 <- r13
	mem rsp 0 <- r12
	r12 <- rsi
	r15 <- rdi
	r14 <- rdi
	r13 <- rdx
	:loop_start
	cjump r12 <= r13 :calc
	goto :ret
	:calc
	rdi <- r12
	mem rsp -8 <- :ily_ret
	call @is_leap_year 1
	:ily_ret
	cjump 1 = rax :leap
	goto :not_leap
	:not_leap
	goto :continue_loop
	:leap
	rdi <- r12
	rdi <<= 1
	rdi++
	r15 += 8
	mem r15 0 <- rdi
	:continue_loop
	r12 += 1
	goto :loop_start
	:ret
	rax <- r14
	r12 <- mem rsp 0
	r13 <- mem rsp 8
	r14 <- mem rsp 16
	r15 <- mem rsp 24
	return
)

(@is_leap_year
	1 0
	rsi <- rdi
	rsi &= 15
	cjump 0 = rsi :yes
	goto :may_not_leap
	:may_not_leap
	rsi <- rdi
	rsi >>= 2
	:loop_mod_25
	cjump 25 = rsi :no
	goto :minus_25
	:minus_25
	cjump 25 < rsi :loop_minus_25
	goto :may_leap
	:loop_minus_25
	rsi -= 25
	goto :loop_mod_25
	:may_leap
	rsi <- rdi
	rsi &= 3
	cjump 0 = rsi :yes
	goto :no
	:yes
	rax <- 1
	return
	:no
	rax <- 0
	return
)

)
