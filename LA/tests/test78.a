void main(){
	:entry
	int64[][] A
	int64[][] B
	int64 Asize
	int64 Bsize
	int64 temp
	int64 temp2
	int64 check1
	int64 check2
	int64 v1
	int64 v2
	int64 v3
	
	A <- new Array(5,5)
	B <- new Array(5,5)
	A[0][0] <- 5
	A[0][1] <- 5
	A[1][0] <- 5
	A[1][1] <- 5

	B[0][0] <- 5
	B[0][1] <- 5
	B[1][0] <- 5
	B[1][1] <- 5

	Asize <- length A 1
	Bsize <- length B 1
	temp <- -1
	temp2 <- 0

	br :outerloop
	:outerloop
	temp <- temp + 1
	temp2 <- 0
	check1 <- temp = Asize
	br check1 :finish :innerloop
	:innerloop
	check2 <- temp2 = Bsize
	br check2 :outerloop :continue
	:continue
	
	v1 <- A[temp][temp2]
	v2 <- B[temp][temp2]
	v3 <- v1 + v2
	A[temp][temp2] <- v3
	temp2 <- temp2 + 1
	br :innerloop
	:finish
	temp <- A[0][0]
	print(temp)
	temp <- A[0][1]
	print(temp)
	temp <- A[1][0]
	print(temp)
	temp <- A[1][1]
	print(temp)
	return
}

