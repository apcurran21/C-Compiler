// how many prime numbers there are in [0, n)?

void main() {
    int64 res
    res <- primeCount(120)

    // 30
    print(res) 
    return
}

int64 primeCount(int64 n) {
    int64[] isPrime
    isPrime <- new Array(n)

    int64 i
    i <- 0
    br :initializeFor
    
    :initializeFor
        int64 if
        if <- i < n
        br if :initForInto :intoForBreak
        
        :initForInto
        isPrime[i] <- 1

        i <- i + 1
        br :initializeFor
        
    :intoForBreak
    int64 res
    res <- 0
    i <- 2

    :for

        if <- i < n
        br if :forInto :forBreak

        :forInto
        int64 v
        v <- isPrime[i]
        if <- v
        br if :isPrime :forContinue
        
        :isPrime
        res <- res + 1
        int64 j
        j <- i * 2
        
        :innerFor
            if <- j < n
            br if :innerForInto :innerForBreak
            
            :innerForInto
            isPrime[j] <- 0
            j <- j + i
            br :innerFor
            
        :innerForBreak
        :forContinue
        i <- i + 1
        br :for
        
    :forBreak
    return res
}
