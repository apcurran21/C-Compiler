// how many odd numbers there are in [0, n]?

void main() {
    int64 n
    int64 res
    int64 i
    n <- 120
    res <- 0
    i <- 0

    :while

        int64 if
        if <- i <= n
        br if :whileInto :whileBreak
        
        :whileInto
        if <- isOdd(i)
        br if :count :whileContinue

        :count
        res <- res + 1
        br :whileContinue
        
        :whileContinue
        i <- i + 1
        br :while
        
        :whileBreak
        // 60
        print(res) 

    return
}

int64 isOdd(int64 a) {
    a <- a >> 1
    a <- a & 1
    int64 if
    if <- a = 1
    br if :yes :no
    
    :no
    return 0

    :yes
    return 1
}
