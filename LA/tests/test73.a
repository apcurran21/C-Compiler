// compute average of 4 numbers

void main() {
    int64 res
    res <- sum(1, 3, 5, 7)
    res <- divide(res)

    // 4
    print(res) 
    return
}

int64 sum(int64 a, int64 b, int64 c, int64 d) {
    int64 res
    res <- a
    res <- res + b
    res <- res + c
    res <- res + d
    return res
}

int64 divide(int64 a) {
    int64 res
    res <- a >> 2
    return res
}
