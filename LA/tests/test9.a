void main ( ){

  int64[][] a
  a <- new Array(2,2)
  initArray(a)
  print(a)
  printE(a, 0, 0)
  printE(a, 0, 1)
  printE(a, 1, 0)
  printE(a, 1, 1)
  return 

}

void initArray (int64[][] A){

  A[0][0] <- 0
  A[0][1] <- 1
  A[1][0] <- 2
  A[1][1] <- 3
  return

}

void printE (int64[][] A, int64 i, int64 j){

  int64 e
  e <- A[i][j]
  print(e)
  return 

}
