void main () {
	int64[] array
	array <- new Array(5)
	int64 print1
	int64 print2
	print1 <- findLen(array)
	print2 <- findSum(array)
	print(print1)
	print(print2)
	return
}
int64 findSum (int64[] array) {
	int64 ret
	int64 temp
	ret <- array[0]
	temp <- array[1]
	ret <- ret + temp
	ret <- ret << 1
	ret <- ret + 1
	return ret

}

int64 findLen (int64[] array) {
	int64 ret
	ret <- length array 0
	ret <- ret << 1
	ret <- ret + 1
	return ret
}