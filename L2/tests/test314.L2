(@main
  (@main
    0
    %num_triangular_nums <- 21
    rdi <- %num_triangular_nums
    call print 1
    rdi <- %num_triangular_nums

mem rsp -8 <- :call_label0
    call @compute_triangular_num 1

:call_label0
    rdi <- %num_triangular_nums

mem rsp -8 <- :call_label1
    call @compute_many_tns 1

:call_label1
    return
  )
  (@compute_triangular_num
    1
    %nums <- rdi
    %decoded_nums <- %nums
    %decoded_nums >>= 1
    %temp_1 <- %decoded_nums
    %temp_1 -= 1
    %temp_1 <- %temp_1
    %temp_1 *= %decoded_nums
    %temp_1 <- %temp_1
    %temp_1 >>= 1
    %temp_1 <- %temp_1
    %temp_1 <<= 1
    %temp_1 <- %temp_1
    %temp_1 += 1
    rdi <- %temp_1
    call print 1
    return
  )
  (@compute_many_tns
    1
    %nums <- rdi
    %decoded_nums <- %nums
    %decoded_nums >>= 1
    %nth_tn <- 3
    %nth_tn <- %nth_tn
    %nth_tn >>= 1
    :call_label_ciao_2
    %cond <- %decoded_nums < %nth_tn

    cjump %cond = 1 :call_label_ciao_5
    :call_label_ciao_3
    %call_nth_tn <- %nth_tn
    %call_nth_tn <<= 1
    %call_nth_tn <- %call_nth_tn
    %call_nth_tn += 1
    rdi <- %call_nth_tn

mem rsp -8 <- :call_label_ciao_6
    call @compute_triangular_num 1

:call_label_ciao_6
    goto :call_label_ciao_4
    :call_label_ciao_4
    %nth_tn <- %nth_tn
    %nth_tn += 1
    goto :call_label_ciao_2
    :call_label_ciao_5
    return
  )
)
