//// Is 1900 a leap year? No!
(@main
        (@main
0

                %start_year <- 1900
                %end_year <- 2000

                //// create a %array to store the answer
                rdi <- %start_year
                rsi <- %end_year

                mem rsp -8 <- :ca_ret
                call @create_array 2
                :ca_ret

                rdi <- rax
                rsi <- %start_year
                rdx <- %end_year

                mem rsp -8 <- :fa_ret
                call @fill_array 3
                :fa_ret

                rdi <- rax
                call print 1

                return
        )

        (@create_array
2

                %size_of_array <- rsi
                %size_of_array -= rdi

                //// %size_of_array / 4
                %size_of_array >>= 2
                %size_of_array++

                %size_of_array <<= 1
                %size_of_array++

                rdi <- %size_of_array
                rsi <- 1
                call allocate 2

                return
        )

        (@fill_array
3

                %current_year <- rsi
                %array_index <- rdi
                %array <- rdi
                %end_year <- rdx

                :loop_start
                cjump %current_year <= %end_year :calc
goto :ret


                :calc
                rdi <- %current_year

                mem rsp -8 <- :ily_ret
                call @is_leap_year 1
                :ily_ret

                cjump 1 = rax :leap
goto :not_leap


                :not_leap
                goto :continue_loop

                :leap
                %encode_current_year <- %current_year
                %encode_current_year <<= 1
                %encode_current_year++

                %array_index += 8
                mem %array_index 0 <- %encode_current_year

                :continue_loop
                %current_year += 1
                goto :loop_start

                :ret
                rax <- %array
                return
        )

        (@is_leap_year
1

                %current_year <- rdi

                %flag <- %current_year
                //// mod 16
                %flag &= 15
                cjump 0 = %flag :yes
goto :may_not_leap


                :may_not_leap
                %flag <- %current_year
                %flag >>= 2

                :loop_mod_25
                cjump 25 = %flag :no
goto :minus_25


                :minus_25
                cjump 25 < %flag :loop_minus_25
goto :may_leap


                :loop_minus_25
                %flag -= 25
                goto :loop_mod_25

                :may_leap
                %flag <- %current_year
                %flag &=3
                cjump 0 = %flag :yes
goto :no


                :yes
                rax <- 1
                return

                :no
                rax <- 0
                return
        )
)
